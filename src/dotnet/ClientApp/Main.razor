@using ActualChat.UI.Blazor.App
@namespace ActualChat.ClientApp

<App SessionId="@_sessionId" />

@code {
    private string _sessionId = Session.Default.Id.Value;
    [Inject] private ClientAppSettings ClientAppSettings { get; init; } = null!;

    protected override void OnInitialized()
    {
        _sessionId = ClientAppSettings.SessionId;
    }
}
