@using ActualChat.App.Maui.Services;
@using Microsoft.AspNetCore.Components.Authorization
@using Stl.Fusion.Blazor

<ActualChat.UI.Blazor.App.App SessionId="@SessionId" />

@code {
    [Inject] NavigationManager Nav { get; init; } = null!;
    [Inject] NavigationInterceptor NavInterceptor { get; init; } = null!;

    [Parameter] public string SessionId { get; set; } = Symbol.Empty;

    protected override void OnInitialized()
    {
        NavInterceptor.Initialize(Nav);
    }
}
